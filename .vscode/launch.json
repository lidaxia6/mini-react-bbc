  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "name": "http://localhost:3000/",
      "request": "launch",
      "url": "http://localhost:3000/",
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**",
        // 调试时 排除以下路径下的文件
        "${workspaceFolder}/node_modules/**",
        "${workspaceFolder}/l-react/node_modules/**"
      ],
    }
  ]
}
